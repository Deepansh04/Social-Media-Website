    <h5>
        <%=flash.success%>
    </h5>
    <%}%> -->
   <main id="layout-main">
  <%- body %>
   </main>
    <%- include('_footer');%>
    <%- script %>
    <script>
         <% if(flash.success && flash.success.length>0) { %>
            new Noty({
                theme:'relax',// relax theme cone preloded
                text:"  <%=flash.success%>",
                type:'success',// color theme
                layout:'topRight',// location of sucess msg
                timeout: 1500// in millisec it goes out


            }).show();

    
            
        <%}%> 
        <% if(flash.error && flash.error.length>0) { %>
            new Noty({
                theme:'relax',// relax theme cone preloded
                text:"  <%=flash.error%>",
                type:'error',// color theme
                layout:'topRight',// location of sucess msg
                timeout: 1500// in millisec it goes out


            }).show();

    
            
        <%}%> 
    
    </script>
</body>
</html>
